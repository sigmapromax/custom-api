<mapper namespace="com.seed.customapi.resource.mapper.ResourceMapper">
    <select id="listByProjId" resultType="com.seed.customapi.resource.entity.ResourceEntity">
        SELECT * FROM resources
        WHERE proj_id = #{projId}
